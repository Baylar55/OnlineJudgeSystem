                                {
                                    <li>
                                        <a asp-action="login" asp-controller="account">
                                            <i class="fas fa-sign-in-alt mr-1"></i><span class="dn-lg">Sign In</span>
                                        </a>
                                    </li>
                                    <li class="add-listing theme-bg">
                                        <a asp-action="register" asp-controller="account" class="text-white">Get Started</a>
                                    </li>
                                }
                            }
                            <li>
                                <div style="border-radius:10px;
                                            padding: 10px;
                                            background-color: #ffecd0;
                                            color: #ffac32;
                                            font-weight: bold;">
                                    <a style="color:#FFA116;" asp-action="index" asp-controller="pricing">Premium</a>
