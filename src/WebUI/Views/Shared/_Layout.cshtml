                                                </div>
                                                <ul>
                                                    <li>
                                                        <a asp-action="index" asp-controller="dashboard">
                                                            <i class="fa fa-tachometer-alt"></i>Dashboard<span class="notti_coun style-1">4</span>
                                                        </a>
                                                    </li>
                                                    <li>
                                                        <a asp-action="index" asp-controller="submissions">
                                                            <i class="fa fa-envelope"></i>Submissions<span class="notti_coun style-3">3</span>
                                                        </a>
                                                    </li>
                                                    <li><a asp-action="index" asp-controller="sessions"><i class="fas fa-comments"></i>Sessions</a></li>
