                            {
                                <li class="@ActivePageHelper.IsActive(ViewContext, "Dashboard")">
                                    <a asp-action="index" asp-controller="dashboard">Dashboard</a>
                                </li>
                            }

                        </ul>

                        <ul class="nav-menu nav-menu-social align-to-right">
                            @{
                                if (User.Identity.IsAuthenticated)
                                {
                                    <li class="account-drop">
                                        <div class="btn-group account-drop">
                                            <a href="javascript:void(0);" class="crs_yuo12 btn btn-order-by-filt"
                                               data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                <img src="~/assets/img/user-1.jpg" class="avater-img" alt="">
